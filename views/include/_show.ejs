<% for(var i=0;i<users.length;i++){ %>
<tr>
    <td><%= i+1  %></td>
    <td><%= users[i].name  %></td>
    <td><%= users[i].email  %></td>
    <td><%= users[i].gender  %></td>
    <td><%= users[i].status  %></td>
    <td>
        <a href="/update-user?id=<%= users[i]._id  %>" class="btn border-shawdow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt" aria-hidden="true"></i></span>
        </a>
        <a class="btn border-shawdow delete" data-id="<%= users[i]._id  %>">
            <span class="text-gradient"><i class="fas fa-times" aria-hidden="true"></i></span>
        </a>
    </td>
</tr>
<% } %>